{
    "Place a horizontal separator": {
        "prefix": "sep",
        "body": [
            "// ----------------------------------------------------------------------------"
        ]
    },
    "Place a segment line with a name": {
        "prefix": "segment",
        "body": [
            "// ----------------------------------------------------------------------------\n// ${name}:"
        ]
    },
    "Place a big segment line": {
        "prefix": "bigsegment",
        "body": [
            "// ============================================================================\n// ==== # ${name}:"
        ]
    },
    "Debug print": {
        "prefix": "deb",
        "body": [
            "console.log(${value});"
        ]
    }
}
